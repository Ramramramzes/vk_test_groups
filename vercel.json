{
  "version": 2,
  "routes": [
    { "src": "/api/server", "dest": "api/server.js" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/build/$1" }
  ],
  "functions": { "api/**/*.js": { "runtime": "nodejs14.x" } },
  "postbuild": {
    "commands": [
      "npm run server"
    ]
  }
}
